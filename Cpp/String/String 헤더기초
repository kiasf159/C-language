#include <iostream>
#include <string>
#pragma warning(disable:4996)
using namespace std;

int main()
{
	string str1 = "I like ";
	string str2 = "string class";
	string str3 = str1 + str2;

	cout << str1 << endl;
	cout << str2 << endl;
	cout << str3 << endl;

	str1 = str1 + str2;
	if (str1 == str3)
		cout << "Same String ! " << endl;
	else
		cout << "Non-Same String ! " << endl;

	string str4;
	cout << "Input String";
	cin >> str4;
	cout << "Input String : " << str4 << endl;
	return 0;
}
