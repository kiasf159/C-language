// What is Template?
// 자료형을 알아서 결정짓도록 만드는 것
// 함수나 클래스를 만들 수 있는 틀

#include "pch.h"
#include <iostream>
using namespace std;

template <typename T>

T Add(T num1, T num2)
{
	return num1 + num2;
}

int main()
{
	cout << Add(15, 20) << endl;
	cout << Add(2.9, 3.1) << endl;
	cout << Add(3.2, 3.2) << endl;
	cout << Add(3.14, 2.75) << endl;
}
