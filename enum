#include <stdio.h>
enum day {sunday, monday, tuesday, wednesday, thrusday, friday, saturday}; // {}cnt; = struct
//typedef enum day wday
//wday cnt;

int main()
{
	enum day cnt;
	char *d_name[] = { "일", "월", "화", "수", "목", "금", "토" };
	for (cnt = sunday; cnt <= saturday; cnt++)
		printf("%s요일\n", d_name[cnt]);
}
